{
  "name": "ts-express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev": "tsnd -P ./tsconfig.json -r tsconfig-paths/register --respawn ./src/server.ts",
    "build": "tsc && tsc-alias",
    "build:sdk": "npx openapi-generator-cli generate -i public/openapi.json -g typescript-axios -o sdk --skip-validate-spec --additional-properties=ngVersion=6.1.7,supportsES6=true,modelPackage=models,apiPackage=api,withSeparateModelsAndApi=true,withInterfaces=true",
    "lint": "eslint --ignore-path .eslintignore ./src",
    "lint:fix": "eslint --ignore-path .eslintignore ./src --fix",
    "test": "nyc --reporter=lcov mocha --require ts-node/register ./src/test/*.test.ts",
    "migrate:g": "npx typeorm-ts-node-esm migration:generate ./src/migrations/mysql/migration -d ./src/datasources/mysql.datasource.ts",
    "migrate:s": "npx typeorm-ts-node-esm migration:run -d ./src/datasources/mysql.datasource.ts",
    "migrate:r": "npx typeorm-ts-node-esm migration:revert -d ./src/datasources/mysql.datasource.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "serve-static": "^1.15.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "triple-beam": "^1.3.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.3",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/triple-beam": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "json-server": "^0.17.0",
    "mocha": "^10.0.0",
    "mockjs": "^1.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
